{
  "name": "sync-version",
  "version": "0.0.0",
  "description": "Update version number with package.json",
  "keywords": [
    "build tool"
  ],
  "bin": "sync-version.js",
  "scripts": {
	"test": "eslint *.js",
    "sync-version": "node sync-version.js sync-version.js",
