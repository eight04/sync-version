    "url": "https://github.com/eight04/sync-version/issues",
    "email": "eight04@gmail.com"
  },
  "homepage": "https://github.com/eight04/sync-version#readme",
  "dependencies": {
    "docopt": "^0.6.2"
  },
  "devDependencies": {
    "eslint": "^2.7.0"
  },
  "preferGlobal": true
}
